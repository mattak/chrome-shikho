{
  "name": "Shikiho Support",
  "version": "0.0.1",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon_32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://shikiho.jp/*"
      ],
      "js": [
        "content_scripts.js"
      ]
    }
  ]
}
